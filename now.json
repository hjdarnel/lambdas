{
    "version": 2,
    "name": "lambda",
    "alias": "lambda.darnell.io",
    "builds": [
        { "src": "*.js", "use": "@now/node" },
        { "src": "beers/*.js", "use": "@now/node" }
    ],
    "routes": [
        { "src": "/beers/interactive", "dest": "/beers/interactive.js" },
        { "src": "/beers/command", "dest": "/beers/command.js" },
        { "src": "/beers/formatBeers.js", "dest": "/beers/formatBeers.js" }
    ],
    "github": {
        "silent": true
    },
    "env": {
        "SLACK_TOKEN": "@beer_slack_token",
        "UPTOWN_API_KEY": "@uptown-api-key",
        "DOWNTOWN_API_KEY": "@downtown-api-key"
    }
}
